38 lines (36 sloc)  793 Bytes
  
pipeline {
  agent any
 
  tools {nodejs "node"}
 
  stages {
    stage('Cloning Git') {
      steps {
        git 'https://github.com/asthagaur1/angular.git' 
		cd angular7tutorial
      }
    }
    stage('Checkout') {
        steps {
            checkout([$class: 'GitSCM', 
        branches: [[name: '*/ui']], 
        doGenerateSubmoduleConfigurations: false, 
        extensions: [], 
        submoduleCfg: [], 
        userRemoteConfigs: [[url: 'https://github.com/asthagaur1/angular.git']]])   
        }
    }
	 
	 stage('Install dependencies') {
      steps {
        bat 'npm install'
      }
    }
    stage('Build') {
      steps {
        bat 'npm run build'
      }
    }
     stage('Test') {
      steps {
        bat 'npm run test-headless'
      }
    }
  }
}
